<h1>Edit Profile</h1>

<%= form_for @user do |f| %>

  <div class="field">
    <%= f.label :first_name, 'First Name' %>
    <%= f.text_field :first_name %>
  </div>

  <br>

  <div class="field">
    <%= f.label :last_name, 'Last Name' %>
    <%= f.text_field :last_name %>
  </div>

  <br>

  <%= f.submit "Update Profile" %>

<% end %>

<br>

<%= link_to 'Back', :back %>
