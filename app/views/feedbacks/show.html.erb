<p>
  <strong>User:</strong>
  <%= @feedback.user.first_name + " " + @feedback.user.last_name %>
</p>

<p>
  <strong>Team:</strong>
  <%= @feedback.team_id %>
</p>

<p>
  <strong>Responded:</strong>
  <%= @feedback.responded %>
</p>

<table>
  <thead>
    <tr>
      <th>Timestamp</th>
      <th>Rating</th>
      <th>Priority</th>
      <th>Comments</th>
      <th>Collaboration and Engagement</th>
      <th>Communication</th>
      <th>Team Support</th>
      <th>Responsibility</th>
      <th>Work Quality</th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td><%=@feedback.timestamp.strftime('%Y-%m-%d %H:%M EST') %></td>
        <td><%= @feedback.rating %></td>
        <%if @feedback.priority == 2%>
          <td><%='Low'%></td>
        <%elsif @feedback.priority == 1%>
          <td><%='Medium'%></td>
        <%else%>
          <td><%='Urgent'%></td>
        <%end%>
        <%if @feedback.comments.size == 0%>
          <td>Students Has Not Submitted Comments!</td>
        <%else%>
          <td><%= @feedback.comments %></td>
        <%end%>  
        <% if @feedback.collab_comment.size != 0%>
          <td><%= @feedback.collaboration %>: <%= @feedback.collab_comment %></td>
        <%else%>
          <td><%= @feedback.collaboration %></td>
        <%end%>
        <% if @feedback.communication_comment.size != 0 %>
          <td><%= @feedback.communication %>: <%= @feedback.communication_comment %></td>
        <%else%>
          <td><%= @feedback.communication %></td>
        <%end%>
        <% if @feedback.team_support_comment.size != 0%>
          <td><%= @feedback.team_support %>: <%= @feedback.team_support_comment %></td>
        <%else%>
          <td><%= @feedback.team_support %></td>
        <%end%>
        <% if @feedback.responsibility_comment.size != 0%>
          <td><%= @feedback.responsibility %>: <%= @feedback.responsibility_comment %></td>
        <%else%>
          <td><%= @feedback.responsibility %></td>
        <%end%> 
        <% if @feedback.work_quality_comment.size != 0%>
          <td><%= @feedback.work_quality_comment %>: <%= @feedback.work_quality_comment %></td>
        <%else%>
          <td><%= @feedback.work_quality %></td>
        <%end%>   
      </tr>
  </tbody>
</table>
<%= link_to 'Back', :back %>
